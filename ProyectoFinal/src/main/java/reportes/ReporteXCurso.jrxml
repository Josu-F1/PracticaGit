	<parameter name="curso" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT 
    c.id_curso,
    c.nombre_curso,
    e.estcedula,
    e.estnombre,
    e.estapellido,
    e.estdireccion,
    e.esttelefono,
    e.estgenero
FROM cursos c
INNER JOIN matriculas m ON c.id_curso = m.id_cur_per
INNER JOIN estudiantes e ON m.estcedula_per = e.estcedula
WHERE c.nombre_curso = $P{curso}
ORDER BY e.estapellido, e.estnombre;]]>
	</queryString>
