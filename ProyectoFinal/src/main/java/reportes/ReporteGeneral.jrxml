	<queryString>
		<![CDATA[SELECT 
    e.estcedula AS Cedula,
    e.estnombre AS Nombre,
    e.estapellido AS Apellido,
    e.estdireccion AS Direccion,
    e.esttelefono AS Telefono,
    e.estgenero AS Genero,
    c.nombre_curso AS Curso
FROM 
    estudiantes e
INNER JOIN 
    matriculas m ON e.estcedula = m.estcedula_per
INNER JOIN 
    cursos c ON m.id_cur_per = c.id_curso;]]>
	</queryString>
