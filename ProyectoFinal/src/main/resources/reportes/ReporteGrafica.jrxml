	<queryString>
		<![CDATA[SELECT 
    c.nombre_curso AS curso,
    e.estgenero AS genero,
    COUNT(e.estcedula) AS cantidad
FROM cursos c
INNER JOIN matriculas m ON c.id_curso = m.id_cur_per
INNER JOIN estudiantes e ON m.estcedula_per = e.estcedula
GROUP BY c.nombre_curso, e.estgenero
ORDER BY c.nombre_curso, e.estgenero;]]>
	</queryString>
